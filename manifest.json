{
  "manifest_version": 3,
  "name": "ChatGPT Remove Feedback",
  "description": "Removes the thumbs up/down feedback buttons on ChatGPT.",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*",
        "https://chatgpt.com/*"
      ],
      "js": [
        "src/content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
